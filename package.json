{
  "name": "comet-starter",
  "version": "1.0.0",
  "description": "React and TypeScript Copilot app, pre-configured with the Comet component library and other tools.",
  "license": "Apache-2.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && rm -rfv $(Agent.BuildDirectory)/s/YourProjectName/apps/portal/node_modules",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "format": "npx prettier src --write",
    "predeploy": "vite build --base=/comet-starter/",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@metrostar/comet-extras": "1.3.1",
    "@metrostar/comet-uswds": "2.0.2",
    "@tanstack/react-query": "5.18.1",
    "@uswds/uswds": "3.7.1",
    "axios": "1.6.7",
    "axios-mock-adapter": "1.22.0",
    "cors": "^2.8.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.50.0",
    "react-oidc-context": "^2.3.1",
    "react-router-dom": "6.21.3",
    "recoil": "0.7.7"
  },
  "devDependencies": {
    "@babel/preset-env": "7.23.9",
    "@babel/preset-react": "7.23.3",
    "@babel/preset-typescript": "7.23.3",
    "@types/react": "18.2.17",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "6.19.1",
    "@typescript-eslint/parser": "6.19.1",
    "@vitejs/plugin-react": "4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.5",
    "gh-pages": "^6.1.1",
    "lint-staged": "15.2.0",
    "prettier": "3.0.0",
    "prettier-plugin-organize-imports": "3.2.4",
    "sass": "1.70.0",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.1.6",
    "vite": "^5.0.12",
    "vite-plugin-environment": "1.1.3",
    "vite-plugin-eslint": "1.8.1",
    "vite-tsconfig-paths": "4.3.1"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.9.6"
  },
  "overrides": {
    "xml2js": "0.6.2"
  }
}
